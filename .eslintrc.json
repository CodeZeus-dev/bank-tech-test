{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true,
        "jasmine": true
    },
    "extends": [
        "prettier",
        "eslint:recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "standard": {
      "parser": "babel-eslint"
    }
}
